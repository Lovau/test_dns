{
  "extends": [
    "eslint:recommended", 
    "plugin:react/recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "react/prop-types": 0,
    "react/no-array-index-key": 1,
    "react/jsx-key": 1,
    "quotes": 0,
    "react/no-unescaped-entities":0,
    "react/display-name": 0,
    "react/no-array-index-key": 0,
    "react/react-in-jsx-scope": 0
  }
}
